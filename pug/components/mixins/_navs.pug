block append variables
  -
    var pageHeader = {
      title: 'Navs',
      description: "Documentation and examples of how to use Bootstrapâ€™s included navigation components.",
      url: `https://getbootstrap.com/docs/${bootstrapVersion}/components/navs/`,
      linkText: 'Navs on Bootstrap'
    }

mixin navs
  ul.nav&attributes(attributes)
    li.nav-item
      a.nav-link.active(href='#') Active
    li.nav-item
      a.nav-link(href='#') Link
    li.nav-item
      a.nav-link(href='#') Link
    li.nav-item
      a.nav-link.disabled(href='#') Disabled

mixin navsFill
  ul.nav.nav-pills.nav-fill
    li.nav-item
      a.nav-link.active(href='#') Active
    li.nav-item
      a.nav-link(href='#') Longer nav link
    li.nav-item
      a.nav-link(href='#') Link